{
  "name": "jsfive",
  "version": "0.3.0",
  "description": "A pure javascript HDF5 file reader, based on pyfive",
  "main": "nodejs/index.js",
  "module": "index.js",
  "browser": "dist/hdf5.js",
  "scripts": {
    "build": "parcel build browser_index.js --target browser",
    "build_node": "parcel build index.js --target node"
  },
  "targets": {
    "browser": {
      "context": "browser",
      "outputFormat": "global"
    },
    "node": {
      "context": "node",
      "distDir": "nodejs",
      "engines": {
        "node": "^8.0.0"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usnistgov/jsfive"
  },
  "keywords": [
    "hdf5",
    "javascript",
    "es6",
    "browser"
  ],
  "author": "Brian B. Maranville",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/usnistgov/jsfive/issues"
  },
  "homepage": "https://github.com/usnistgov/jsfive#readme",
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "pako-es": "^1.0.9"
  },
  "devDependencies": {
    "parcel": "^2.0.0-beta.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
